<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CRUD_Personas_MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CRUD_Personas_MAUI.Views"
    Shell.FlyoutBehavior="Disabled">

    <FlyoutItem>

        <ShellContent Title="Personas" ContentTemplate="{DataTemplate local:ListadoPersona}"/>
   

        <ShellContent Title="Departamentos" ContentTemplate="{DataTemplate local:ListadoDepartamento}"/>
    </FlyoutItem>

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"/>


</Shell>
